<h1>Song: <%= @song.name %></h1>
  <% if !!flash[:message] %> <%= flash[:message] %> <% end %>

  <div class="song">
    <h2>Song Name: <%= @song.name %></h2>
    <h4>Song Artist: <a href="/artists/<%= @song.artist.slug %>"><%= @song.artist.name %><a/></h4>
    <h4>Song Genre(s): <%= @song.genres.map {|g| "<a href=/genres/"+g.slug+">"+g.name+"</a>"}.to_s %></h4>
    <a href="/songs/<%= @song.slug %>/edit">Edit</a>
  </div><br>
