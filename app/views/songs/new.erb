<h1>Add New Song</h1>

<form action="/songs" method="POST">

    <label>Name:</label>
    <br></br>
    <input type="text" name="song_name]" id="Name">
    <br></br>
    <label>Artist Name:</label>
    <br></br>
    <input type="text" name="artist_name" id="Artist Name" >
    <br></br>s

    <label>Choose genre(s):</label>
    <br></br>
    <%Genre.all.each do |genre|%>
      <input type="checkbox" name="genre_ids[]" id="<%=genre.name%>" value="<%=genre.id%>"><%=genre.name%></input>
    <%end%>

      <br></br>

  <input type="submit" value="Create">
</form>
